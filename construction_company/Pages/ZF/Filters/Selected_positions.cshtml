@page
@model construction_company.Pages.ZF.Filters.Selected_positionsModel
@{
}

<h1>Отдел кадров</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Staff[0].FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Staff[0].positionID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Staff)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FullName)
                </td>
                <td>
                    @Model.position.Name_Position

                </td>
            </tr>
        }
    </tbody>
</table>